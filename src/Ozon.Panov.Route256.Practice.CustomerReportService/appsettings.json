{
  "Kestrel": {
    "Endpoints": {
      "WebApi": {
        "Url": "http://*:5001",
        "Protocols": "Http1"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimitSettings": {
    "ReportServiceRateLimit": 2,
    "OrderServiceRateLimit": 20
  },
  "ORDER_SERVICE_URL": "http://localhost:8084/"
}
